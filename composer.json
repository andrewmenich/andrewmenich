{
  "require": {
    "craftcms/aws-s3": "1.2.11",
    "craftcms/cms": "3.6.16",
    "craftcms/feed-me": "4.3.6",
    "craftcms/redactor": "2.8.7",
    "nystudio107/craft-imageoptimize": "1.6.33",
    "nystudio107/craft-seomatic": "3.3.42",
    "nystudio107/craft-vite": "1.0.7",
    "putyourlightson/craft-sprig": "1.5.2",
    "vlucas/phpdotenv": "^3.4.0"
  },
  "require-dev": {
    "yiisoft/yii2-shell": "^2.0.3"
  },
  "autoload": {
    "psr-4": {
      "modules\\": "modules/",
      "sprig\\components\\": "sprig/components/"
    }
  },
  "config": {
    "sort-packages": true,
    "optimize-autoloader": true,
    "platform": {
      "php": "7.4"
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ]
  }
}
