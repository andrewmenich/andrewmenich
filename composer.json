{
  "require": {
    "craftcms/aws-s3": "2.0.3",
    "craftcms/ckeditor": "3.7.1",
    "craftcms/cms": "^4.0.0",
    "craftcms/feed-me": "5.3.0",
    "craftcms/redactor": "3.0.4",
    "nystudio107/craft-imageoptimize": "4.0.5",
    "nystudio107/craft-seomatic": "4.0.38",
    "nystudio107/craft-vite": "4.0.8",
    "putyourlightson/craft-sprig": "2.7.3",
    "vlucas/phpdotenv": "^5.3.0"
  },
  "require-dev": {
    "yiisoft/yii2-shell": "^2.0.3"
  },
  "autoload": {
    "psr-4": {
      "modules\\": "modules/",
      "sprig\\components\\": "sprig/components/"
    }
  },
  "config": {
    "sort-packages": true,
    "optimize-autoloader": true,
    "platform": {
      "php": "8.2"
    },
    "allow-plugins": {
      "yiisoft/yii2-composer": true,
      "craftcms/plugin-installer": true
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ]
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://composer.craftcms.com",
      "canonical": false
    }
  ]
}